{
  "LibDb": {
    "ConnectionStrings": {
      "Default": "Server=127.0.0.1;Database=LIBDB_VERIFICATION_TEST;User Id=user_id;Password=user_password;TrustServerCertificate=True;Encrypt=False;",
    },
    "EnableSchemaCaching": true,
    "SchemaRefreshIntervalSeconds": 60,
    "PrewarmSchemas": [
      "dbo"
    ],
    "PrewarmIncludePatterns": [],
    "PrewarmExcludePatterns": [
      "*_Test*",
      "*_Legacy*"
    ],
    "PrewarmMaxConcurrency": 0,
    "StrictRequiredParameterCheck": true,
    "EnableGeneratedTvpBinder": true,
    "EnableResilience": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Lib.Db": "Debug"
    }
  }
}